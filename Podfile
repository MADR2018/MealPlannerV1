# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FBApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Auth'

  # Pods for FBApp

  target 'FBAppTests' do
    inherit! :search_paths
    # Pods for testing
  pod 'Firebase/Core'
  pod 'Firebase/Auth'

  end

  target 'FBAppUITests' do
    inherit! :search_paths
    # Pods for testing
  pod 'Firebase/Core'
  pod 'Firebase/Auth'

  end

end
